class Motor:
    def __init__(self, tipo, potencia):
        self.tipo = tipo
        self.potencia = potencia

class Carro:
    def __init__(self, motor):
        self.motor = motor

motor_gasolina = Motor("Gasolina", 150)
carro1 = Carro(motor_gasolina)

motor_eletrico = Motor("Elétrico", 200)
carro2 = Carro(motor_eletrico)

print(f"Carro 1 - Motor: Tipo: {carro1.motor.tipo}, Potência: {carro1.motor.potencia} cavalos")
print(f"Carro 2 - Motor: Tipo: {carro2.motor.tipo}, Potência: {carro2.motor.potencia} cavalos")
